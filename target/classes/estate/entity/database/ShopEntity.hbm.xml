<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.ShopEntity" table="shop" schema="" catalog="PropertySystem">
        <id name="id">
            <column name="id" sql-type="int unsigned" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="propertyId">
            <column name="property_id" sql-type="int unsigned"/>
        </property>
        <property name="code">
            <column name="code" sql-type="varchar" length="45"/>
        </property>
        <property name="villageId">
            <column name="village_id" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>