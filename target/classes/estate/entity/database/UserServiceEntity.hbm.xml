<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.UserServiceEntity" table="user_service" schema="" catalog="estate">
        <id name="usId">
            <column name="usID" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="userId">
            <column name="userID" sql-type="int unsigned"/>
        </property>
        <property name="serviceId">
            <column name="serviceID" sql-type="int unsigned"/>
        </property>
        <property name="createTime">
            <column name="create_time" sql-type="varchar" length="15"/>
        </property>
        <property name="payStatus">
            <column name="payStatus" sql-type="int"/>
        </property>
        <property name="serviceStatus">
            <column name="serviceStatus" sql-type="int"/>
        </property>
        <property name="comment">
            <column name="comment" sql-type="varchar" length="100"/>
        </property>
        <many-to-one name="serviceByServiceId" update="false" insert="false" class="estate.entity.database.ServiceEntity">
            <column name="serviceID"/>
        </many-to-one>
        <many-to-one name="userByUserId" update="false" insert="false" class="estate.entity.database.UserEntity">
            <column name="userID"/>
        </many-to-one>
    </class>
</hibernate-mapping>