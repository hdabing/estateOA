<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.SsidSecretEntity" table="ssid_secret" schema=""
           catalog="PropertySystem">
        <id name="id">
            <column name="sc_id" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="buildingId">
            <column name="building_id" sql-type="int unsigned"/>
        </property>
        <property name="ssid">
            <column name="ssid" sql-type="varchar" length="45"/>
        </property>
        <property name="secret">
            <column name="secret" sql-type="varchar" length="45"/>
        </property>
    </class>
</hibernate-mapping>