<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.NoticeEntity" table="notice" schema="" catalog="estate">
        <id name="niticeId">
            <column name="niticeID" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar" length="50"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar"/>
        </property>
        <property name="detail">
            <column name="detail" sql-type="text" length="65535"/>
        </property>
        <property name="createTime">
            <column name="createTime" sql-type="varchar" length="15"/>
        </property>
        <property name="expireTime">
            <column name="expireTime" sql-type="varchar" length="15"/>
        </property>
        <property name="picPath">
            <column name="picPath" sql-type="varchar" length="255"/>
        </property>
        <property name="creater">
            <column name="creater" sql-type="varchar" length="25"/>
        </property>
    </class>
</hibernate-mapping>