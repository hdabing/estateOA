<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.PictureEntity" table="picture" schema="" catalog="PropertySystem">
        <id name="pictureId">
            <column name="picture_id" sql-type="int unsigned" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="45"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar" length="45"/>
        </property>
        <property name="dir">
            <column name="dir" sql-type="varchar" length="100"/>
        </property>
    </class>
</hibernate-mapping>