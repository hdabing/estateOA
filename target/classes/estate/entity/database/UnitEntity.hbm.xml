<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.UnitEntity" table="unit" schema="" catalog="estate">
        <id name="unitId">
            <column name="unitID" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="unitName">
            <column name="unitName" sql-type="varchar" length="25"/>
        </property>
        <property name="buildingId">
            <column name="buildingID" sql-type="int unsigned"/>
        </property>
        <many-to-one name="buildingByBuildingId" insert="false" update="false" class="estate.entity.database.BuildingEntity">
            <column name="buildingID"/>
        </many-to-one>
        <set name="usersByUnitId" inverse="true">
            <key>
                <column name="unitID"/>
            </key>
            <one-to-many not-found="ignore" class="estate.entity.database.UserEntity"/>
        </set>
    </class>
</hibernate-mapping>