<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.FeeTypeEntity" table="fee_type" schema="" catalog="PropertySystem">
        <id name="ftId">
            <column name="ft_id" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar" length="100"/>
        </property>
        <property name="description">
            <column name="description" sql-type="varchar" length="200"/>
        </property>
    </class>
</hibernate-mapping>