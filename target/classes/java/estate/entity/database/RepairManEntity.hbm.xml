<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.RepairManEntity" table="repair_man" schema="" catalog="PropertySystem">
        <id name="id">
            <column name="rp_id" sql-type="int unsigned" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="phone">
            <column name="phone" sql-type="varchar" length="15" not-null="true"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar" length="45"/>
        </property>
    </class>
</hibernate-mapping>