<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.PropertyOwnerInfoEntity" table="property_owner_info" schema=""
           catalog="PropertySystem">
        <id name="id">
            <column name="po_id" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="propertyId">
            <column name="property_id" sql-type="int unsigned"/>
        </property>
        <property name="ownerPhone">
            <column name="owner_phone" sql-type="varchar" length="15"/>
        </property>
        <property name="buildingId">
            <column name="building_id" sql-type="int unsigned"/>
        </property>
        <property name="openDoorAllowed">
            <column name="open_door_allowed" sql-type="tinyint"/>
        </property>
    </class>
</hibernate-mapping>