<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="estate.entity.database.OpenDoorRecordEntity" table="open_door_record" schema=""
           catalog="PropertySystem">
        <id name="id">
            <column name="id" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="userId">
            <column name="user_id" sql-type="int unsigned"/>
        </property>
        <property name="openTime">
            <column name="open_time" sql-type="bigint"/>
        </property>
        <property name="openCode">
            <column name="open_code" sql-type="varchar" length="100"/>
        </property>
    </class>
</hibernate-mapping>