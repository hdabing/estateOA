<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.database.GroupEntity" table="group" schema="" catalog="estate">
        <id name="groupId">
            <column name="groupID" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="groupName">
            <column name="groupName" sql-type="varchar" length="25"/>
        </property>
        <property name="auth">
            <column name="auth" sql-type="text" length="65535"/>
        </property>
        <set name="adminsByGroupId" inverse="true">
            <key>
                <column name="groupID"/>
            </key>
            <one-to-many not-found="ignore" class="entity.database.AdminEntity"/>
        </set>
    </class>
</hibernate-mapping>