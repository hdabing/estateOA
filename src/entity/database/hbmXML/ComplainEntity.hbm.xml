<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.database.ComplainEntity" table="complain" schema="" catalog="estate">
        <id name="complainId">
            <column name="complainID" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="title">
            <column name="title" sql-type="varchar" length="50"/>
        </property>
        <property name="detail">
            <column name="detail" sql-type="text" length="65535"/>
        </property>
        <property name="picPath">
            <column name="picPath" sql-type="varchar"/>
        </property>
        <property name="createTime">
            <column name="createTime" sql-type="varchar" length="15"/>
        </property>
        <property name="status">
            <column name="status" sql-type="int"/>
        </property>
        <property name="score">
            <column name="score" sql-type="int"/>
        </property>
        <property name="adminId">
            <column name="adminID" sql-type="int unsigned"/>
        </property>
        <property name="userId">
            <column name="userID" sql-type="int unsigned"/>
        </property>
        <many-to-one name="adminByAdminId" class="entity.database.AdminEntity">
            <column name="adminID"/>
        </many-to-one>
        <many-to-one name="userByUserId" class="entity.database.UserEntity">
            <column name="userID"/>
        </many-to-one>
    </class>
</hibernate-mapping>