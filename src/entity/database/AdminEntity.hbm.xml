<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="entity.database.AdminEntity" table="admin" schema="" catalog="estate">
        <id name="adminId">
            <column name="adminID" sql-type="int unsigned" not-null="true"/>
        </id>
        <property name="username">
            <column name="username" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar" length="45" not-null="true"/>
        </property>
        <property name="salt">
            <column name="salt" sql-type="varchar" length="45"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar" length="45"/>
        </property>
        <property name="groupId">
            <column name="groupID" sql-type="int unsigned"/>
        </property>
        <property name="lastLogin">
            <column name="lastLogin" sql-type="varchar" length="15"/>
        </property>
        <many-to-one name="groupByGroupId" class="entity.database.GroupEntity">
            <column name="groupID"/>
        </many-to-one>
        <set name="complainsByAdminId" inverse="true">
            <key>
                <column name="adminID"/>
            </key>
            <one-to-many not-found="ignore" class="entity.database.ComplainEntity"/>
        </set>
    </class>
</hibernate-mapping>